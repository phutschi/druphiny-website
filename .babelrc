{
  "plugins": [
    [
      "module-alias",
      [{ "src": "./src/modules/layout", "expose": "layout" }],
      [{ "src": "./src/modules/page", "expose": "page" }],
      [{ "src": "./src/modules/posts", "expose": "posts" }],
      [{ "src": "./src/modules/products", "expose": "products" }],
      [{ "src": "./src/modules/stats", "expose": "stats" }]
    ]
  ],
  "env": {
    "development": {
      "presets": "next/babel"
    },
    "production": {
      "presets": "next/babel"
    },
    "env": {
      "development": {
        "plugins": ["inline-dotenv"]
      },
      "production": {
        "plugins": ["transform-inline-environment-variables"]
      }
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ],
        "next/babel"
      ]
    }
  }
}
