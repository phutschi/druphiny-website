{
  "env": {
    "development": {
      "presets": "next/babel"
    },
    "production": {
      "presets": "next/babel"
    },
    "env": {
      "development": {
        "plugins": ["inline-dotenv"]
      },
      "production": {
        "plugins": ["transform-inline-environment-variables"]
      }
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ],
        "next/babel"
      ]
    }
  }
}
