{
  "plugins": [
    [
      "module-alias",
      [{ "src": "./src/modules/layout/components", "expose": "UI/Layout" }],
      [{ "src": "./src/modules/page/components", "expose": "UI/Page" }],
      [{ "src": "./src/modules/posts/components", "expose": "UI/Posts" }],
      [{ "src": "./src/modules/products/components", "expose": "UI/Products" }],
      [{ "src": "./src/modules/stats/components", "expose": "UI/Stats" }]
    ]
  ],
  "env": {
    "development": {
      "presets": "next/babel"
    },
    "production": {
      "presets": "next/babel"
    },
    "env": {
      "development": {
        "plugins": ["inline-dotenv"]
      },
      "production": {
        "plugins": ["transform-inline-environment-variables"]
      }
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ],
        "next/babel"
      ]
    }
  }
}
